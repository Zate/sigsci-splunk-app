{
    "meta": {
        "name": "sigsci_TA_for_splunk", 
        "apiVersion": "3.0.0", 
        "version": "1.0.17", 
        "restRoot": "sigsci_TA_for_splunk", 
        "displayName": "sigsci_TA_for_splunk"
    }, 
    "pages": {
        "configuration": {
            "title": "Configuration", 
            "tabs": [
                {
                    "name": "proxy", 
                    "title": "Proxy", 
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }, 
                    "entity": [
                        {
                            "label": "Enable", 
                            "type": "checkbox", 
                            "field": "proxy_enabled"
                        }, 
                        {
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "http", 
                                        "value": "http"
                                    }, 
                                    {
                                        "label": "socks4", 
                                        "value": "socks4"
                                    }, 
                                    {
                                        "label": "socks5", 
                                        "value": "socks5"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "label": "Proxy Type", 
                            "type": "singleSelect", 
                            "field": "proxy_type", 
                            "defaultValue": "http"
                        }, 
                        {
                            "validators": [
                                {
                                    "errorMsg": "Max host length is 4096", 
                                    "minLength": 0, 
                                    "maxLength": 4096, 
                                    "type": "string"
                                }
                            ], 
                            "label": "Host", 
                            "type": "text", 
                            "field": "proxy_url"
                        }, 
                        {
                            "validators": [
                                {
                                    "type": "number", 
                                    "range": [
                                        1, 
                                        65535
                                    ]
                                }
                            ], 
                            "label": "Port", 
                            "type": "text", 
                            "field": "proxy_port"
                        }, 
                        {
                            "validators": [
                                {
                                    "errorMsg": "Max length of username is 50", 
                                    "minLength": 0, 
                                    "maxLength": 50, 
                                    "type": "string"
                                }
                            ], 
                            "label": "Username", 
                            "type": "text", 
                            "field": "proxy_username"
                        }, 
                        {
                            "validators": [
                                {
                                    "errorMsg": "Max length of password is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ], 
                            "encrypted": true, 
                            "label": "Password", 
                            "type": "text", 
                            "field": "proxy_password"
                        }, 
                        {
                            "label": "Remote DNS resolution", 
                            "type": "checkbox", 
                            "field": "proxy_rdns"
                        }
                    ]
                }, 
                {
                    "name": "logging", 
                    "title": "Logging", 
                    "entity": [
                        {
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "DEBUG", 
                                        "value": "DEBUG"
                                    }, 
                                    {
                                        "label": "INFO", 
                                        "value": "INFO"
                                    }, 
                                    {
                                        "label": "WARNING", 
                                        "value": "WARNING"
                                    }, 
                                    {
                                        "label": "ERROR", 
                                        "value": "ERROR"
                                    }, 
                                    {
                                        "label": "CRITICAL", 
                                        "value": "CRITICAL"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "label": "Log level", 
                            "type": "singleSelect", 
                            "field": "loglevel", 
                            "defaultValue": "INFO"
                        }
                    ]
                }, 
                {
                    "name": "additional_parameters", 
                    "title": "Add-on Settings", 
                    "entity": [
                        {
                            "field": "email", 
                            "help": "This is the e-mail of the user for ", 
                            "type": "text", 
                            "required": true, 
                            "defaultValue": "", 
                            "label": "SigSci E-mail", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ], 
                            "options": {
                                "placeholder": "user@domain.com"
                            }
                        }, 
                        {
                            "field": "password", 
                            "encrypted": true, 
                            "help": "Password for the SigSci User, if API Token is used this field IS NOT used.", 
                            "type": "text", 
                            "required": false, 
                            "defaultValue": "", 
                            "label": "Password", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of password is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ]
                        }, 
                        {
                            "field": "apitoken", 
                            "encrypted": true, 
                            "help": "API Token associated with the SigSci User", 
                            "type": "text", 
                            "required": false, 
                            "defaultValue": "", 
                            "label": "API Token", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of password is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ]
                        }, 
                        {
                            "field": "corp", 
                            "help": "This is the Corp API Name", 
                            "type": "text", 
                            "required": true, 
                            "defaultValue": "", 
                            "label": "SigSci Corp", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "description": "Set up your add-on"
        }, 
        "inputs": {
            "title": "Inputs", 
            "services": [
                {
                    "name": "SigsciRequests", 
                    "title": "SigSci Requests", 
                    "entity": [
                        {
                            "field": "name", 
                            "help": "Enter a unique name for the data input", 
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex"
                                }, 
                                {
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "minLength": 1, 
                                    "maxLength": 100, 
                                    "type": "string"
                                }
                            ], 
                            "required": true, 
                            "label": "Name", 
                            "type": "text"
                        }, 
                        {
                            "field": "index", 
                            "label": "Index", 
                            "type": "singleSelect", 
                            "required": true, 
                            "defaultValue": "default", 
                            "validators": [
                                {
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "minLength": 1, 
                                    "maxLength": 80, 
                                    "type": "string"
                                }
                            ], 
                            "options": {
                                "endpointUrl": "data/indexes", 
                                "blackList": "^_.*$", 
                                "createSearchChoice": true
                            }
                        }, 
                        {
                            "help": "This is a numeric value and is recommended to leave at 5", 
                            "field": "delta", 
                            "required": true, 
                            "defaultValue": "5", 
                            "label": "Delta", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ], 
                            "options": {
                                "placeholder": "Time frame to pull data"
                            }, 
                            "type": "text"
                        }, 
                        {
                            "field": "site", 
                            "help": "This is the API Name of the Dashboard Site", 
                            "required": true, 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ], 
                            "label": "Dashboard Site Name", 
                            "options": {
                                "placeholder": "Dashboard Site Name"
                            }, 
                            "type": "text"
                        }
                    ]
                }, 
                {
                    "name": "SigsciEvent", 
                    "title": "Sigsci Event", 
                    "entity": [
                        {
                            "field": "name", 
                            "help": "Enter a unique name for the data input", 
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex"
                                }, 
                                {
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "minLength": 1, 
                                    "maxLength": 100, 
                                    "type": "string"
                                }
                            ], 
                            "required": true, 
                            "label": "Name", 
                            "type": "text"
                        }, 
                        {
                            "field": "index", 
                            "label": "Index", 
                            "type": "singleSelect", 
                            "required": true, 
                            "defaultValue": "default", 
                            "validators": [
                                {
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "minLength": 1, 
                                    "maxLength": 80, 
                                    "type": "string"
                                }
                            ], 
                            "options": {
                                "endpointUrl": "data/indexes", 
                                "blackList": "^_.*$", 
                                "createSearchChoice": true
                            }
                        }, 
                        {
                            "help": "Time frame to pull data from Signal Sciences", 
                            "field": "delta", 
                            "required": true, 
                            "defaultValue": "5", 
                            "label": "Delta", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ], 
                            "type": "text"
                        }, 
                        {
                            "field": "site", 
                            "help": "API Dashboard Site Name, this IS NOT a http url", 
                            "required": true, 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ], 
                            "label": "Dashboard Site Name", 
                            "options": {
                                "placeholder": "Dashboard Site API Name"
                            }, 
                            "type": "text"
                        }
                    ]
                }
            ], 
            "table": {
                "header": [
                    {
                        "label": "Name", 
                        "field": "name"
                    }, 
                    {
                        "label": "Interval", 
                        "field": "interval"
                    }, 
                    {
                        "label": "Index", 
                        "field": "index"
                    }, 
                    {
                        "label": "Status", 
                        "field": "disabled"
                    }
                ], 
                "moreInfo": [
                    {
                        "label": "Name", 
                        "field": "name"
                    }, 
                    {
                        "label": "Interval", 
                        "field": "interval"
                    }, 
                    {
                        "label": "Index", 
                        "field": "index"
                    }, 
                    {
                        "label": "Status", 
                        "field": "disabled"
                    }, 
                    {
                        "label": "Delta", 
                        "field": "delta"
                    }, 
                    {
                        "label": "Dashboard Site Name", 
                        "field": "site"
                    }
                ], 
                "actions": [
                    "edit", 
                    "enable", 
                    "delete", 
                    "clone"
                ]
            }, 
            "description": "Manage your data inputs"
        }
    }
}
